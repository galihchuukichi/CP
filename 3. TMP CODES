// ELITE YELLOW #02 INTERACTIVE INPUT
#include<bits/stdc++.h>
using namespace std;
using ll = long long;
#define endl "\n"
#define aku ios_base::sync_with_stdio(0);cout.tie(0);cin.tie(0);
#define fup(i,a,b,c) for(ll i=a; i<=b; i+=c)
#define fdown(i,a,b,c) for(ll i=a; i>=b; i-=c)
#define fr first
#define sc second
#define vll vector<ll>
#define pll pair<ll,ll>
#define pb emplace_back
#define pf emplace_front


ll tc,x,y,m,n,a,b,p,q,ans,sum;

void solve(){ // cout.flush() tiap output
    cin>>n;
    ll arr[n+3],suff[n+3]={};
    fup(i,1,n,1) {cin>>arr[i];suff[i]=suff[i-1]+arr[i];}
    // INPUT DONE
    
    ll l=1,r=n,brt;
    while(l!=n){
        ll mid = (l+r+1)/2;
        cout<<"? "<<mid-l+1<<" ";
        fup(i,l,mid,1) cout<<i<<" ";
        cout.flush();
        
        cin>>brt;
        if(brt==suff[mid]-suff[l-1]) r = mid;
        else l = mid+1;
        
        cout<<"l : "<<l<<" | r : "<<r<<endl; cout.flush();
    }
    cout<<l<<endl; cout.flush();
}

signed main(){
    aku
    cin>>tc;
    // tc=1;
    while(tc--) solve();
}
